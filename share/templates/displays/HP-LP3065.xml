<?xml version="1.0" ?>

<!--

LP3065 is HP's top-end 30" LCD monitor (at the time of writing this).

The LP 3065 supports resolutions upto 2560x1600 @ 60Hz.

-->

<displayconfig 
 xmlns="http://www.hp.com"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.hp.com /opt/vizstack/share/schema/displayconfig.xsd" >
	<model>LP3065</model>
	<vendor>HP</vendor>
	<input>digital</input>
	<edid>/opt/vizstack/share/templates/displays/edids/HP-LP3065.bin</edid>
	<default_mode>2560x1600_60</default_mode>
	<mode>
		<type>edid</type>
		<alias>2560x1600_60</alias>
		<alias>2560x1600</alias>
		<width>2560</width>
		<height>1600</height>
		<refresh>60</refresh>
	</mode>
	<mode>
		<type>edid</type>
		<alias>1280x800_60</alias>
		<alias>1280x800</alias>
		<width>1280</width>
		<height>800</height>
		<refresh>60</refresh>
	</mode>
</displayconfig>
